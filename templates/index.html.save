	<div class="spacing"></div>
		<h2 class="center">Events</h2>

<!--<h5>Events of 2017</h5>-->

<ul class="collapsible popout" data-collapsible="accordion" style="color:#222;">
{% for e in event %}
	{% if e.start_time[0:4] == "2017" %}
        <li>
        <div class="collapsible-header" >{{ e.name }} - {{ e.start_time[0:10] }}</div>
		<div class="collapsible-body" style="background: white;">
		<span>
			<b>Date :</b> {{ e.start_time[0:10] }} <br>
			<b>Description :</b> {{ e.description }} <br>
			<b>Image :</b> <br>
			<img class="responsive-img" src='{{ e.cover["source"] }}'>
		</span>
		</div>        
		</li>
	{% endif %}
{% endfor %}
</ul>


